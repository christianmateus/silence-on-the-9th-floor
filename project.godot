config_version=5

[application]

config/name="Silence on the 9th Floor"
run/main_scene="res://Scenes/Cutscenes/intro.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

